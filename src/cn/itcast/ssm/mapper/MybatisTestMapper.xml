<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.itcast.ssm.mapper.MybatisTestMapper" >

	<select id="getList" resultType="cn.itcast.ssm.po.Test">
		select * from test
		
	</select>
	
	<insert id="save" parameterType="cn.itcast.ssm.po.Test">
		insert into test(aaa,bbb,ccc,ddd,eee)values(#{aaa},#{bbb},#{ccc},#{ddd},#{eee})
	</insert>
</mapper>